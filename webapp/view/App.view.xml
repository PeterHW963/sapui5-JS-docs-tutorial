<mvc:View
    controllerName="ui5.walkthrough.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true">
    <!-- we don't add Shell control to XML view if apps run in external shell, such as Fiori Launchpad that alr has a shell around the component UI -->
    <Shell>
        <App class="myAppDemoWT" id="app"/> <!-- we add this class just to make sure the css selector that has .myAppDemoWT is only used by this app -->
    </Shell>
</mvc:View>